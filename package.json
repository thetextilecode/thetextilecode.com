{
  "name": "openthreads",
  "version": "0.1.0-dev",
  "private": true,
  "scripts": {
    "deploy": "git push origin main && vercel --prod",
    "dev": "next dev",
    "build": "next build",
    "format": "prettier --write \"**/*.{mdx,tsx,ts,md,js,jsx,json}\"",
    "format-watch": "npm run format && onchange \"**/*.{mdx,tsx,ts,md,js,jsx,json}\" -- prettier --write {{changed}}",
    "lint": "next lint",
    "lint:fix": "npm run lint -- --fix",
    "lint-staged": "npm run lint:fix && npm run format:fix",
    "postbuild": "next-sitemap",
    "start": "next start",
    "typecheck": "tsc",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "bootstrap": "^5.3.2",
    "date-fns": "^4.1.0",
    "fontfaceobserver": "^2.3.0",
    "isomorphic-unfetch": "^4.0.2",
    "next": "^13.5.11",
    "next-mdx-remote": "^5.0.0",
    "next-sitemap": "^4.2.3",
    "react": "^19.2.3",
    "react-bootstrap": "^2.8.0",
    "react-content-loader": "^7.1.1",
    "react-dom": "^19.2.3",
    "react-img-zoom": "^0.1.0",
    "react-input-range": "^1.3.0",
    "react-mailchimp-subscribe": "^2.1.3",
    "react-netlify-forms": "^2.0.3",
    "react-perfect-scrollbar": "^1.5.8",
    "react-redux": "^9.2.0",
    "react-responsive-modal": "^7.1.0",
    "react-slick": "^0.31.0",
    "react-toast-notifications": "^2.5.1",
    "react-toastify": "^11.0.5",
    "reactstrap": "^9.2.1",
    "redux": "^4.2.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^3.1.0",
    "select2": "^4.1.0-rc.0",
    "sharp": "^0.34.5",
    "slick-carousel": "^1.8.1",
    "swiper": "^12.0.3",
    "wowjs": "^1.1.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/node": "^25.0.6",
    "@types/react": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^13.5.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.0.0",
    "gray-matter": "^4.0.3",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0"
  }
}
